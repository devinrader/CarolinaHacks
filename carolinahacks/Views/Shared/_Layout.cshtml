<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>CarolinaHacks: @ViewBag.Title</title>
    @Styles.Render("~/Content/themes/base/css", "~/Content/bootstrap", "~/Content/css", "~/Content/fontawesome")
    
    @Scripts.Render("~/bundles/jquery", "~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
